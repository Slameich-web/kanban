{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "requireConfigFile": false,
    "root": true,
    "parser": "@babel/eslint-parser",
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": "error"
    }
}
